{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.12.0",
  "scripts": {
    "postinstall": "husky install",
    "next-ts": "pnpm -F=@example/next-ts",
    "utility": "pnpm -F=@example/utility"
  },
  "dependencies": {
    "lodash-es": "^4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1",
    "@commitlint/cli": "^18",
    "@commitlint/config-conventional": "^18",
    "@types/node": "^20",
    "husky": "^8",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=8.12.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "msw": {
    "workerDirectory": "packages/next-ts/public"
  },
  "lint-staged": {
    "**.{js|ts|cjs|mjs|d.cts|d.mts|jsx|tsx|json|jsonc}": [
      "biome check --files-ignore-unknown=true",
      "biome check --apply --no-errors-on-unmatched",
      "biome check --apply --organize-imports-enabled=false --no-errors-on-unmatched",
      "biome check --apply-unsafe --no-errors-on-unmatched",
      "biome format --write --no-errors-on-unmatched",
      "biome lint --apply --no-errors-on-unmatched"
    ],
    "*": [
      "biome check --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  }
}
