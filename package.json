{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next-ts": "pnpm -F=@example/next-ts",
    "vite-react": "pnpm -F=@example/vite-react",
    "utility": "pnpm -F=@example/utility",
    "lint:staged": "oxlint",
    "fmt:staged": "oxfmt --write",
    "lint": "oxlint .",
    "fmt": "oxfmt --write .",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^20",
    "@commitlint/config-conventional": "^20",
    "@commitlint/cz-commitlint": "^20",
    "@commitlint/types": "^20",
    "@types/node": "^24",
    "commitizen": "^4",
    "lefthook": "^2",
    "oxfmt": "^0",
    "oxlint": "^1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=24.12.0",
    "npm": "please-use-pnpm",
    "pnpm": ">=10.26.2",
    "yarn": "please-use-pnpm"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "lefthook",
      "msw",
      "oxlint",
      "sharp"
    ]
  }
}
