{
  "name": "vite-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:script": "eslint --ext \".js,.jsx,.vue,.ts,.tsx\" --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --ignore-path .gitignore",
    "lint": "yarn lint:script && yarn lint:style"
  },
  "packageManager": "yarn@1.22.15",
  "dependencies": {
    "@vitejs/plugin-vue-jsx": "^1.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "vue": "^3.2.16",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "msw": "^0.35.0",
    "postcss": "^8.4.3",
    "prettier": "^2.5.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-pages": "^0.18.2",
    "vue-tsc": "^0.3.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "eslint",
    "*.{css,scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
