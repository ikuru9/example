{
  "name": "vite-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts",
    "build:no-cache": "pnpm clean:cache && npm run build",
    "serve": "vite preview",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:style",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mocks}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:style": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:markup": "vue-tsc --noEmit",
    "cert": "rm .cert && mkdir -p .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem 'localhost'",
    "gen:icon": "esno ./build/generate/icon/index.ts"
  },
  "packageManager": "pnpm@6.31.0",
  "dependencies": {
    "@vueuse/core": "^7.6.2",
    "@vueuse/shared": "^7.6.2",
    "axios": "^0.26.0",
    "dayjs": "^1.10.7",
    "pinia": "^2.0.11",
    "qs": "^6.10.3",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@iconify/vue": "^3.1.3",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "chalk": "^5.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "msw": "^0.36.8",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.49.8",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.5.5",
    "unocss": "^0.25.0",
    "vite": "^2.8.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.0.6",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pages": "^0.20.2",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^0.31.4"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "eslint",
    "*.{css,scss,vue}": "stylelint",
    "*": "prettier -w -u"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
