{
  "name": "vite-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts",
    "build:no-cache": "pnpm clean:cache && npm run build",
    "preview": "vite preview",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:style",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mocks}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:style": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:markup": "vue-tsc --noEmit",
    "cert": "rm .cert && mkdir -p .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem 'localhost'",
    "gen:icon": "esno ./build/generate/icon/index.ts"
  },
  "packageManager": "yarn@3.2.2",
  "dependencies": {
    "@vueuse/core": "^9.1.0",
    "@vueuse/shared": "^9.1.0",
    "axios": "^0.27.2",
    "dayjs": "^1.11.4",
    "pinia": "^2.0.17",
    "qs": "^6.11.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify/vue": "^3.2.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "inquirer": "^9.1.0",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.54.3",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "typescript": "^4.5.0",
    "unocss": "^0.45.5",
    "vite": "^3.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-pwa": "^0.12.3",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^0.39.5"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "eslint",
    "*.{css,scss,vue}": "stylelint",
    "*": "prettier -w -u"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
